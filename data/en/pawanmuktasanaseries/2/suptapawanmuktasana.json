{
    "data": {
        "header": "Supta Pawanmuktasana",
        "header_en": "Leg Lock Pose",
        "startingPosition": {
            "header": "Starting Position",
            "img": "",
            "instruction": "Lie flat on the back. Legs together and straight. Arms by the sides and palms down. Head, neck and spine in a straight line."
        },
        "stage": [
            {
                "id": "1",
                "img": "/images/pawanmuktasanaseries/2/suptapawanmuktasana1.png",
                "technique": [
                    {
                        "id": "1",
                        "img": "",
                        "instruction": "Bend the right knee bringing the right thigh to the chest and interlocking the fingers hold the shin just below the right knee."
                    },
                    {
                        "id": "2",
                        "img": "",
                        "instruction": "Inhale deeply."
                    },
                    {
                        "id": "3",
                        "img": "",
                        "instruction": "Exhaling raise the head and shoulders off the ground, and bring the right knee to the nose."
                    },
                    {
                        "id": "4",
                        "img": "",
                        "instruction": "Hold for 3 to 5 seconds, retaining the breath."
                    },
                    {
                        "id": "5",
                        "img": "",
                        "instruction": "Inhaling slowly return to the staring position."
                    }
                ],
                "info": [
                    "Repeat 3 times."
                ]
            },
            {
